ninja_required_version = 1.3
cxx = c++
nvcc = /lusr/cuda/bin/nvcc

cflags = -pthread -B /scratch/cluster/lzchen/anaconda3/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /scratch/cluster/lzchen/anaconda3/include -I/scratch/cluster/lzchen/anaconda3/include -fPIC -O2 -isystem /scratch/cluster/lzchen/anaconda3/include -fPIC -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/TH -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/THC -I/lusr/cuda/include -I/scratch/cluster/lzchen/anaconda3/include/python3.9 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/TH -I/scratch/cluster/lzchen/anaconda3/lib/python3.9/site-packages/torch/include/THC -I/lusr/cuda/include -I/scratch/cluster/lzchen/anaconda3/include/python3.9 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_70,code=compute_70 -gencode=arch=compute_70,code=sm_70 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/build/temp.linux-x86_64-3.9/ext.o: compile /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/ext.cpp
build /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/build/temp.linux-x86_64-3.9/simple_knn.o: cuda_compile /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/simple_knn.cu
build /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/build/temp.linux-x86_64-3.9/spatial.o: cuda_compile /scratch/cluster/lzchen/lzchen_vscode/gaussian-splatting/submodules/simple-knn/spatial.cu







